public class PostsDocManager {

	static final String PREFIX = "PostDoc";
	Map<String,int> docTable = null;
	
	PostsDocManager() {
		docTable = new LinkedHashMap<String,int>();
	}
	
	private String getDynamicFieldName(String parentId) {
		
	}
	
	private String getFileName(String Id) {
		return PREFIX + Id;
	}
}
